            <!-- jade Specific Keywords -->
            <keywords id="jade Specific Keywords" color="#060" inverted-color="#CFC">
                <regex>^\s*(append)\b</regex>
                <regex>^\s*(block)\b</regex>
                <regex>^\s*(case)\b</regex> 
                <regex>^\s*(each)\b</regex>  
                <regex>^\s*(extends)\b</regex> 
                <regex>^\s*(for)\b</regex>  
                <regex>^\s*(if)\b</regex>   
                <regex>^\s*(include)\b</regex>    
                <regex>^\s*(mixin)\b</regex> 
                <regex>^\s*(when)\b</regex>           
            </keywords>
            
            <state id="jade ID" color="#881280" inverted-color="#fac586"  font-style="normal" font-weight="normal" useforautocomplete="no">  
            <!-- color="#d6771c" -->
                <begin><regex>#(?=\w)</regex></begin>
                <end><regex>[\w-]*(?=[^\w-]*)</regex></end>
              <!-- <end><regex>\W{1}</regex></end> -->
            </state>
            <state id="jade Class" color="#881280" inverted-color="#fac586"  font-style="normal" font-weight="normal" useforautocomplete="no">  
            <!-- color="#d6771c" -->
                <begin><regex>\.(?=\w)</regex></begin>
                <end><regex>[\w-]*(?=[^\w-]*)</regex></end>
              <!-- <end><regex>\W{1}</regex></end> -->
            </state>
            
            <state id="jade HTML Attributes" color="#586E75" inverted-color="#93A1A1">
                <begin><regex>\(</regex></begin>
                <end><regex>\)</regex></end>
                
                <state id="Strings in jade HTML Attributes" type="string" color="#2AA198" inverted-color="#2AA198" useforautocomplete="no">
                    <begin><regex>"</regex></begin>
                    <end><regex>"</regex></end>
                </state>
                
                <!-- STUDIO support for single quote strings -->
                <state id="Single Strings in jade HTML Attributes" type="string" color="#2AA198" inverted-color="#2AA198" useforautocomplete="no">
                	<begin><regex>'</regex></begin>
                    <end><regex>'</regex></end>
                </state>
                
                <keywords id="Attributes in jade HTML Attributes" color="#586E75" inverted-color="#93A1A1" useforautocomplete="no">
                    <regex>([a-zA-Z]{1}[\w\-\:]*)(?=[ \t]*=)</regex>
                </keywords>
                
            </state>
            
            <state id="jade Embedded Javascript" color="#000" inverted-color="#FFF">
                <begin><regex>\A\s*(?:-|=|!=)</regex></begin>
                <end><regex>.$</regex></end>

                <keywords id="JS Keywords" color="#881350" > 
                    <regex>\b(if)\b</regex>
                    <regex>\b(else)\b</regex>
                    <regex>\b(for)\b</regex>
                    <regex>\b(in)\b</regex>
                    <regex>\b(while)\b</regex>
                    <regex>\b(do)\b</regex>
                    <regex>\b(continue)\b</regex>
                    <regex>\b(break)\b</regex>
                    <regex>\b(with)\b</regex>
                    <regex>\b(try)\b</regex>
                    <regex>\b(catch)\b</regex>
                    <regex>\b(switch)\b</regex>
                    <regex>\b(case)\b</regex>
                    <regex>\b(new)\b</regex>
                    <regex>\b(var)\b</regex>
                    <regex>\b(function)\b</regex>
                    <regex>\b(return)\b</regex>
                    <regex>\b(this)\b</regex>
                    <regex>\b(delete)\b</regex>
                    <regex>\b(true)\b</regex>
                    <regex>\b(false)\b</regex>
                    <regex>\b(void)\b</regex>
                    <regex>\b(throw)\b</regex>
                    <regex>\b(typeof)\b</regex>
                    <regex>\b(const)\b</regex>
                    <regex>\b(default)\b</regex>
                </keywords>
                
                <state-link mode="Javascript" state="AutodocComment" />
                
                <state-link mode="Javascript" state="Comment" />
                
                <state-link mode="Javascript" state="Regular Expressions" />
                
                <state-link mode="Javascript" state="String" />
                
                <state-link mode="Javascript" state="SingleString" />

            </state>

            <state id="jade Interpolate" color="#003" inverted-color="#CCF" >
                <begin><regex>(?:#\{)</regex></begin>
                <end><regex>\}(?&lt;!\\)</regex></end>
            </state>

            <state id="jade Interpolate Unescaped" color="#F00" inverted-color="#FCC" >
                <begin><regex>(?:[\\]{0}\!\{)</regex></begin>
                <end><regex>\}(?&lt;!\\)</regex></end>
            </state>

            <state id="jade Comment" color="#236E25" font-style="italic" useforautocomplete="yes" type="comment">
                <begin><regex>^\s*(?:\/\/)</regex></begin>
                <end><regex>[\r\n]</regex></end>
            </state>
            
            <state id="jade Doctype" color="#236E25" font-style="italic">
                <begin><regex>\A(?:\!\!\!)</regex></begin>
                <end><regex>[\r\n]</regex></end>
            </state>

